      <Text layoutX="511.0" layoutY="154.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Expense">
         <font>
            <Font name="Serif Regular" size="48.0" />
         </font>
      </Text>
      <TextField fx:id="ExpenseTotal" layoutX="490.0" layoutY="230.0" promptText="Expense Total ($)">
         <font>
            <Font name="Serif Regular" size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="ExpenseName" layoutX="489.0" layoutY="320.0" promptText="Name of Expense">
         <font>
            <Font name="Serif Regular" size="24.0" />
         </font>
      </TextField>
      <Button fx:id="AddExpenseButton" layoutX="518.0" layoutY="518.0" mnemonicParsing="false" onAction="#addExpenseToAcc" text="Add Expense">
         <font>
            <Font name="Serif Regular" size="36.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="expenseType" layoutX="489.0" layoutY="410.0" prefHeight="44.0" prefWidth="300.0" />
