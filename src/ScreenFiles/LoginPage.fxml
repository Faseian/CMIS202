<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ScreenFiles.ScreenControllers.LoginSignupController">
   <children>
      <Text layoutX="583.0" layoutY="220.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
         <font>
            <Font name="Serif Regular" size="48.0" />
         </font>
      </Text>
      <TextField fx:id="userField" layoutX="490.0" layoutY="300.0" promptText="Username">
         <font>
            <Font name="Serif Regular" size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="passField" layoutX="490.0" layoutY="400.0" promptText="Password">
         <font>
            <Font name="Serif Regular" size="24.0" />
         </font>
      </TextField>
      <Button layoutX="564.0" layoutY="500.0" mnemonicParsing="false" onAction="#checkLogin" text="Submit">
         <font>
            <Font name="Serif Regular" size="36.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="Back">
         <font>
            <Font name="Serif Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
